      <driver>com.mysql.jdbc.Driver</driver>
      <url>jdbc:mysql://<%= mysql_server_1 %>:<%= mysql_port %>/<%= rss_database %>?autoReconnect=true</url>
      <username><%= rss_user %></username>
      <password><%= rss_password %></password>
  </rss-database>

